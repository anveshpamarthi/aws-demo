pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
            print "Build Completed"
            }
        }
        stage('DeployToTest'){
            steps{
            print "Deployed to test"
            }
        }
        stage('DeployToDev'){
            steps{
            print "Deployed to Dev"
            }
        } 
        stage('DeployToQa'){
            steps{
            print "Deployed to Qa"
            }
        }

    }
}
